                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mfrac>
                    <m:mrow>
                      <m:mi> opposite </m:mi>
                      <m:mo>(</m:mo>
                      <m:msup>
                        <m:mn>45</m:mn>
