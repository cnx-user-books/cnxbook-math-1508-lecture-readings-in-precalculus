                            </m:msqrt>
                          </m:mfrac>
                        </m:mfenced>
                        <m:mn>2</m:mn>
                      </m:msup>
                      <m:mo>-</m:mo>
                      <m:mfenced separators="" open="(" close=")">
                        <m:mn>1</m:mn>
                        <m:mo>-</m:mo>
                        <m:mfrac>
