                                <m:mrow>
                                  <m:mo stretchy="false">−</m:mo>
                                  <m:mn>3</m:mn>
                                </m:mrow>
                                <m:mo stretchy="false">≤</m:mo>
                                <m:mi>x</m:mi>
                              </m:mrow>
