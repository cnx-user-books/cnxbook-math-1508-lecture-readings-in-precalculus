<exercise id="prob1exer"><problem id="idm15768432">
      <para id="id9422659"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mfrac><m:mrow><m:mrow><m:msup><m:mn>3x</m:mn><m:mstyle fontsize="8pt"><m:mrow><m:mn>2</m:mn></m:mrow></m:mstyle></m:msup><m:mo stretchy="false">+</m:mo><m:mn>5x</m:mn></m:mrow><m:mo stretchy="false">−</m:mo><m:mn>8</m:mn></m:mrow><m:mrow><m:mn>6x</m:mn><m:mo stretchy="false">+</m:mo><m:mtext>16</m:mtext></m:mrow></m:mfrac></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ {  {3x rSup { size 8{2} } +5x - 8}  over  {6x+"16"} } } {}</m:annotation></m:semantics><m:mo>×</m:mo><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mfrac><m:mrow><m:msup><m:mn>4x</m:mn><m:mstyle fontsize="8pt"><m:mrow><m:mn>3</m:mn></m:mrow></m:mstyle></m:msup><m:mo stretchy="false">+</m:mo><m:mi>x</m:mi></m:mrow><m:mrow><m:msup><m:mn>3x</m:mn><m:mstyle fontsize="8pt"><m:mrow><m:mn>3</m:mn></m:mrow></m:mstyle></m:msup><m:mo stretchy="false">−</m:mo><m:mn>3x</m:mn></m:mrow></m:mfrac></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ {  {4x rSup { size 8{3} } +x}  over  {3x rSup { size 8{3} }  - 3x} } } {}</m:annotation></m:semantics></m:math></para>

      <list id="id11368225" list-type="labeled-item" mark-suffix=".">
<item><label>a</label>Simplify</item>
        <item><label>b</label>What values of <m:math><m:mi>x</m:mi></m:math> are not allowed in the original expression?</item>
        <item><label>c</label>What values of <m:math>
<m:mi>x</m:mi></m:math> are not allowed in your simplified expression?</item>
        </list>
     </problem>
   </exercise>

<exercise id="prob2exer"><problem id="idm16829232">
          <para id="id10486229"><m:math>
              <m:semantics>
                <m:mrow>
