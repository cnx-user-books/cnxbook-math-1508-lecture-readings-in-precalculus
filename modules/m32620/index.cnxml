                    </m:mrow>
                  </m:math>
                </entry>
                <entry>
                  <figure id="id85421">
                    <media id="id85421_media" alt="">
                      <image mime-type="image/png" src="../../media/MG10C15_021.png" id="id85421_onlineimage" width="72"><!-- NOTE: attribute width changes image size online (pixels). original width is 72. --></image>
                      <image mime-type="application/postscript" for="pdf" src="../../media/MG10C15_021.eps" id="id85421_printimage"/>
                    </media>
